{
  "name": "@octommit/cli",
  "description": "cli program to update files in git",
  "version": "1.0.19",
  "author": {
    "name": "Stockopedia"
  },
  "repository": {
    "url": "https://github.com/Stockopedia/octommit"
  },
  "scripts": {
    "test": "jest",
    "debug": "npm run build && npm run start",
    "build": "tsc -p src",
    "start": "node dist/index.js",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "make:badges": "node_modules/.bin/jest-badges-readme",
    "make:readme": "npm run test -- --coverage && npm run make:badges",
    "make:changelog": "changelog"
  },
  "bin": {
    "octommit": "dist/index.js"
  },
  "dependencies": {
    "@octokit/auth-token": "^2.4.2",
    "@octokit/core": "^3.1.3",
    "@octokit/rest": "^18.0.6",
    "@octommit/core": "^1.0.18",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "events": "^3.2.0",
    "http": "0.0.1-security",
    "lodash": "^4.17.20",
    "ncp": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "vorpal": "^1.12.0",
    "winston": "^3.3.3",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@octokit/types": "^5.5.0",
    "@olavoparno/jest-badges-readme": "^1.5.1",
    "@stryker-mutator/core": "^4.0.0",
    "@stryker-mutator/jest-runner": "^4.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.7",
    "@types/faker": "^5.1.3",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@types/vorpal": "^1.12.0",
    "concurrently": "^5.3.0",
    "csv-parse": "^4.12.0",
    "faker": "^5.1.0",
    "generate-changelog": "^1.8.0",
    "graphql-depth-limit": "^1.1.0",
    "husky": "^4.3.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.4.0",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "sqlite3": "^5.0.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.10.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
