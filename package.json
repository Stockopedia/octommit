{
  "name": "@stockopedia/octommit",
  "description": "cli program to update files in git",
  "version": "1.0.0",
  "author": {
    "name": "Stockopedia"
  },
  "repository": {
    "url": "https://github.com/Stockopedia/octommit"
  },
  "scripts": {
    "debug": "npm run build && npm run start",
    "test": "jest",
    "build": "tsc -p src",
    "start": "node dist/cli/index.js",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "prepare": "husky install"
  },
  "bin": "dist/cli/index.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "dependencies": {
    "@octokit/auth-token": "^4.0.0",
    "@octokit/core": "^4.2.1",
    "@octokit/rest": "^19.0.11",
    "@types/lodash": "^4.14.195",
    "@types/vorpal": "^1.12.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.1.4",
    "events": "^3.3.0",
    "http": "0.0.1-security",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "tslint-config-prettier": "^1.18.0",
    "tsyringe": "^4.7.0",
    "vorpal": "^1.12.0",
    "winston": "^3.9.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jest/globals": "^29.5.0",
    "@octokit/types": "^9.3.1",
    "@stryker-mutator/core": "^7.0.2",
    "@stryker-mutator/jest-runner": "^7.0.2",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "faker": "^6.6.6",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
